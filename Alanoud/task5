// Q1
import 'dart:io';

void main() {
  for (int i = 1; i <= 10; i++) {
    stdout.write(i); // prints numbers on the same line
  }
}
// Q2
import 'dart:io';

void main() {
  int sum = 0;

  for (int i = 1; i <= 10; i++) {
    sum = sum + i; // add the current number to sum
  }

  print("The sum of the first 10 natural numbers is $sum");
}

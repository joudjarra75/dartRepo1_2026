void main() {
  print("Hello, in this code we will be using \"Escape characters\"\n"
        "This is backslash \\\n"
        "This is double quotes \"\n"
        "This is single quotes \'\n"
        "we can print new line using \" \\n \"\n"
        "and print spaces using \" \\t \"");
}
// Q2
void main(){
  print("welcom to the company");
  
  stdout.write("Enter your name");
  String name = stdin.readLineSync()!;

  stdout.write("Enter your age");
  int num = int.parse(stdin.readLineSync()!);

  stdout.write("Enter your majour");
  String major = (stdin.readLineSync()!);
  print("welcom to the company");
  print("welcom $name");
  print("your info:");
  print("Name:$name");
  print("age:$num");
  print("Major:$major");
}
